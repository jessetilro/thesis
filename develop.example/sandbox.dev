module test
  model
    entity One {
      x : Boolean
    }
	entity Two {	
	  y : Boolean
	}
	
	relation OneTwo {
		One.onetwo -> one
		Two.onetwo -> two
		
		one.two <-> two.one
		
		test : Boolean
	}
  
data

  test : One {
  	onetwo = <:OneTwo{}> {}
  	two = <{ test = true }> {}
  }
  :Two {
  	one = test
  }
  
execute

	test.two.y