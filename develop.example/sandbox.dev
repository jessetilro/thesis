module test
  model
    entity One extends Two {
    	x : Boolean
    }
	entity Two extends Three {
		y : Boolean = z
	}
	entity Three {
		z : Boolean
	}
	
	relation OneTwo {
		One.onetwo -> one
		Two.onetwo -> two
		
		one.two <-> two.one
		
		test : Boolean
	}
	
functions
  
  round1(f : Float) : Float = (f * 10.0 + 0.5) as Int as Float / 10.0
  
data

  test : One {
  	onetwo = <:OneTwo{}> {}
  	two = <{ test = true }> {}
  }
  :Two {
  	one = test
  }
  
execute

	test.two.y
